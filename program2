def addition(a,b):
    print("The addition is:",a+b)
def subtraction(a,b):
    print("The subtraction is:",a-b)
def multiplication(a,b):
    print("The multiplication is:",a*b)
def division(a,b):
    print("The division is:",a/b)
addition(2,3)
multiplication(5,6)
subtraction(10,2)
division(20,2)
'''-----------------------------OUTPUT----------------------------
The addition is: 5
The multiplication is: 30
The subtraction is: 8
The division is: 10.0
'''
